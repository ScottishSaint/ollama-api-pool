name = "ollama-api-pool"
main = "src/index.js"
compatibility_date = "2025-01-01"

# KV 命名空间
[[kv_namespaces]]
binding = "API_KEYS"
id = "your-api-keys-kv-namespace-id"  # 替换为你的 KV namespace ID

[[kv_namespaces]]
binding = "ACCOUNTS"
id = "your-accounts-kv-namespace-id"  # 替换为你的 KV namespace ID

# 环境变量
[vars]
ADMIN_TOKEN = "your-admin-token-here"  # 管理后台的密钥，建议使用强密码
ENABLE_ANALYTICS = "true"

# 日志配置
[observability.logs]
enabled = false
